package com.game.module.fashion;

/**
 * Created by lucky on 2017/7/3.
 * 时装信息
 */
public class Fashion {
    private int id; //配置ID
    private long createTime; //获取时间
    private int period; //有效时间 0 表示永久

    public Fashion() {
    }

    public Fashion(int id, long createTime, int period) {
        this.id = id;
        this.createTime = createTime;
        this.period = period;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public long getCreateTime() {
        return createTime;
    }

    public void setCreateTime(long createTime) {
        this.createTime = createTime;
    }

    public int getPeriod() {
        return period;
    }

    public void setPeriod(int period) {
        this.period = period;
    }
}
